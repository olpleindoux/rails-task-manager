<div class="container">
  <h2>Create a new task</h2>
  <%= form_for(@task) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :details %>
    <%= f.text_field :details %>
    <%= f.submit %>
  <% end %>
  <p>&nbsp;</p>
  <p><%= link_to 'Cancel', tasks_path %></p>
</div>

